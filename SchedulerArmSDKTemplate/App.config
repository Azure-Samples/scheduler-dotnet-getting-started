<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- See http://blog.davidebbo.com/2014/12/azure-service-principal.html to learn how to obtain these values -->
    
    <add key = "AzureSubscriptionId" value = "[Enter subscriptionId]" />
    <add key = "AzureClientId" value = "[Enter clientId]"/>
    <add key = "AzureTenantId" value = "[Enter tenantId]"/>
    <add key = "AzureClientSecret" value = "[Enter client secret]" />
    
    <add key = "AzureLocation" value = "[Enter region]" />
    <add key = "AzureResourceGroup" value = "[Enter resource group]" />
    <add key = "HttpActionUrl" value = "http://www.bing.com/" />
  </appSettings>
</configuration>